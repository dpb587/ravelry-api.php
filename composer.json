      "email" : "dpb587@gmail.com",
      "homepage" : "https://dpb587.me"
    }
  ],

  "require" : {
    "php" : ">=5.4.0",
    "guzzlehttp/guzzle" : "4.0.2",
    "guzzlehttp/guzzle-services" : "0.2.0",
